{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "es2018",
      "dom",
      "dom.iterable"
    ],
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ]
}